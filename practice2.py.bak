p=int(input("enter the number of rows in matrix 1:"))
q=int(input("enter the number of cols in matrix 2:"))
n=int(input("enter the number of rows in matrix 2 or cols in matrix1:"))

print("enter the values in matrix1:")
matrix1=[[int(input())for i in range(n)]for j in range(p)]

print("matrix1")
for i in range(p):
   for j in range(n):
     print(format(matrix1[i][j],"<4"),end="")
   print()
   
print("enter the values in matrix2:")
matrix2=[[int(input())for i in range(q)]for j in range(n)]

print("matrix1")
for i in range(n):
   for j in range(q):
     print(format(matrix2[i][j],"<4"),end="")
   print() 
  
result[i][j]=[[0for i in range(q)]for j in range(p)]
for i in range(p):
   for j in range(q):
      for k in range(n):
         result[i][j]=result[i][j]+matrix1[i][k]*matrix2[k][j]  

