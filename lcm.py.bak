def compute_LCM(n1,n2):
  if(n1>n2):
     higher = n1
  else:
     higher = n2
   
  value = higher
  while True:
    if(higher%n1==0 and higher%n2==0):
       print("lcm of given numbers:",higher)
       break
    else:
       higher=higher+value
	   
